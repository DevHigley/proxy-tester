{
	"compilerOptions": {
		"strict": true,
		"jsx": "react",
		"allowJs": true,
		"target": "es5",
		"sourceMap": true,
		"outDir": "./dist",
		"skipLibCheck": true,
		"module": "commonjs",
		"esModuleInterop": true,
